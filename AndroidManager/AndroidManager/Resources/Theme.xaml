                    <Color x:Key="SystemAltMediumLowColor">#FF000000</Color>
                    <Color x:Key="SystemBaseHighColor">#FFFFFFFF</Color>
                    <Color x:Key="SystemBaseLowColor">#33544F4F</Color>
                    <Color x:Key="SystemBaseMediumColor">#FFAAA8A8</Color>
                    <Color x:Key="SystemBaseMediumHighColor">#FFC0BEBE</Color>
                    <Color x:Key="SystemBaseMediumLowColor">#FF7F7B7B</Color>
                    <Color x:Key="SystemChromeAltLowColor">#FFC0BEBE</Color>
                    <Color x:Key="SystemChromeBlackHighColor">#FF000000</Color>
                    <Color x:Key="SystemChromeBlackLowColor">#FFC0BEBE</Color>
                    <Color x:Key="SystemChromeBlackMediumColor">#FF000000</Color>
                    <Color x:Key="SystemChromeBlackMediumLowColor">#FF000000</Color>
