                    <Color x:Key="NavigationContentBackground">#FFFCF6F6</Color>
                    <Color x:Key="NavigationBarBackgroud">#FFF8EDED</Color>
                    <Color x:Key="NavigationBarBorderColor">#FFE9DFDF</Color>
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
        <ResourceDictionary x:Key="HighContrast">
            <StaticResource x:Key="RegionColor" ResourceKey="SystemColorWindowColor" />
            <SolidColorBrush x:Key="RegionBrush" Color="{StaticResource RegionColor}" />
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>
